{
  "name": "ns-seo-content-agent",
  "version": "1.0.0",
  "description": "Agentic SEO content pipeline using OpenAI Agents SDK",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts \"Best RV under $25k for 3 people\" --bucket daily",
    "pipeline": "tsx src/index.ts",
    "daily": "tsx src/run/runDaily.ts",
    "weekly": "tsx src/run/runWeekly.ts",
    "monthly": "tsx src/run/runMonthly.ts",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:watch": "vitest --watch",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write src tests",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "seo",
    "content",
    "ai",
    "openai",
    "agents",
    "markdown"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openai/agents": "^0.0.17",
    "@toolsycc/json-repair": "^0.1.22",
    "chalk": "^5.6.0",
    "dotenv": "^16.4.5",
    "gray-matter": "^4.0.3",
    "marked": "^12.0.2",
    "slugify": "^1.6.6",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  }
}
